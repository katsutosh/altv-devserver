{
  "name": "devserver-katsutosh",
  "version": "0.0.1",
  "description": "Devserver",
  "main": "",
  "scripts": {
    "clean": "rimraf ./resources",
    "build": "npm run clean && swc ./src -d ./resources --copy-files",
    "update": "altv-pkg d dev",
    "windows": "npm run build && altv-server.exe",
    "windev": "nodemon --watch \"./src/**\" --ext \"ts\" --exec \"npm run build && altv-server.exe\""
  },
  "author": "katsutosh",
  "license": "ISC",
  "devDependencies": {
    "@altv/types-client": "^2.1.9",
    "@altv/types-natives": "^1.4.4",
    "@altv/types-server": "^2.3.7",
    "@altv/types-shared": "^1.2.6",
    "@altv/types-webview": "^1.0.2",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.127",
    "@types/node": "^14.6.1",
    "altv-pkg": "^2.0.4",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "tsup": "^4.11.2",
    "typescript": "latest"
  },
  "type": "module",
  "dependencies": {
    "request": "^2.88.2"
  }
}
